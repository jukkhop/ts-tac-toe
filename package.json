  "name": "ts-tac-toe",
  "version": "1.0.0",
  "dependencies": {
    "express": "^4.17.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-node": "^4.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3",
    "typescript-bundle": "^1.0.16"
  },
  "scripts": {
    "bundle-src": "tsc-bundle ./src/tsconfig.json",
    "compile-src": "tsc --build ./src/tsconfig.json",
    "compile-server": "tsc --build ./server/tsconfig.json",
    "start": "tsc --build ./server/tsconfig.json && tsc-bundle ./src/tsconfig.json && node ./build-server/server.js"
